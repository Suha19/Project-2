<head>

    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <title>WikiFoodia</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Favicons -->
    <link rel="shortcut icon" href="content/dietitian2/images/favicon.ico">

    <!-- FONTS -->
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:100,300,400,400italic,700'>
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Patua+One:100,300,400,400italic,700'>
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Lato:400,400italic,700,700italic,900'>
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:100,300,400,400italic,500,700,700italic'>
    <link rel='stylesheet'
        href='http://fonts.googleapis.com/css?family=Arima+Madurai:100,300,400,400italic,500,700,700italic'>

    <!-- CSS -->
    <link rel='stylesheet' href='/css/global.css'>
    <link rel='stylesheet' href='/content/dietitian2/css/structure.css'>
    <link rel='stylesheet' href='/content/dietitian2/css/dietitian2.css'>
    <link rel='stylesheet' href='/content/dietitian2/css/custom.css'>

    <!-- Revolution Slider -->
    <link rel="stylesheet" href="plugins/rs-plugin/css/settings.css">

</head>
<div>

</div>
<div id="Subheader">
    <div class="container">
        <div class="column one">
            <h1 class="title">Recipes</h1>
            <!--BreadCrumbs area-->
            <ul class="breadcrumbs">
                <li>
                    <a href="/">Home</a><span><i class="icon-right-open"></i></span>
                </li>
                <li>
                    <a href="api/recipe">All Recipes</a><i class="icon-right-open"></i></span>
                </li>
                <li>
                    <a href="/members">Add Recipes</a>
                </li>
            </ul>
        </div>
    </div>
</div>

<div id="Content">
    <div class="recipe-container">
        <div class="sections_group">
            <div class="entry-content">
                <div class="section mcb-section" style="padding-top:0px; padding-bottom:0px;">
                    <div class="section_wrapper mcb-section-inner">
                        <div class="wrap mcb-wrap one valign-top clearfix">
                            <div class="mcb-wrap-inner">
                                <div class="column mcb-column one column_blog ">
                                    <div class="column_filters">
                                        <div class="blog_wrapper isotope_wrapper clearfix">
                                            <div class="posts_group lm_wrapper col-3 grid">
                                                <div
                                                    class="post-item isotope-item clearfix post type-post format-standard has-post-thumbnail hentry category-classic tag-themeforest">
                                                    <div class="date_label">
                                                        December 7, 2015
                                                    </div>
                                                    {{#each recipes}}
                                                    <div class="post-desc-wrapper">
                                                        <div class="post-desc">
                                                            <div class="post-head">
                                                                <div class="post-meta clearfix">
                                                                    <div class="author-date">
                                                                        <span class="vcard author post-author"><span
                                                                                class="label">Published by </span><i
                                                                                class="icon-user"></i> <span
                                                                                class="fn"><span
                                                                                    class="member-name"></span></h2>
                                                                                <span class="date"><span class="label">
                                                                                        at
                                                                                    </span><i class="icon-clock"></i>
                                                                                    <span class="post-date updated">June
                                                                                        22,
                                                                                        2019</span></span>
                                                                    </div>
                                                                    <br>
                                                                    <div class="mealType">
                                                                        <span class="cat-btn"
                                                                            id="mealType">{{this.recipe_type}}
                                                                        </span>
                                                                    </div>
                                                                </div>
                                                            </div>


                                                            <div class="post-title">
                                                                <h2 class="entry-title" id="recipeName"
                                                                    itemprop="headline">{{this.name_of_meal}}</h2>
                                                            </div>

                                                            <div class="post-excerpt">
                                                                <span class="big"
                                                                    id="recDescrip">{{this.descriptions}}</span>
                                                            </div>
                                                            <div class="post-excerpt">
                                                                <span class="big"
                                                                    id="prep_time">{{this.prep_time}}</span>
                                                            </div>
                                                            <div class="post-excerpt">
                                                                <span class="big"
                                                                    id="caloric_content">{{this.caloric_content}}</span>
                                                            </div>
                                                            <div class="post-excerpt">
                                                                <span class="big" id="servings">{{this.servings}}</span>
                                                            </div>
                                                            <div class="post-excerpt">
                                                                <span class="big"
                                                                    id="ingredients">{{this.ingredients}}</span>
                                                            </div>
                                                            <div class="post-excerpt">
                                                                <span class="big"
                                                                    id="cookInstruct">{{this.cooking_instructions}}</span>
                                                            </div>
                                                            <div class="post-excerpt" id="image">

                                                                <img src="{{this.meal_image}}" height="300" width="300">
                                                            </div>


                                                            <script>
                                                                var button = document.getElementById("clickme"),
                                                                    count = 0;
                                                                button.onclick = function () {
                                                                    count += 1;
                                                                    button.innerHTML = count;
                                                                };
                                                            </script>
                                                            <div class="post-footer">
                                                                <a <div class="button-love">
                                                                    <class="button-love" class="icon-heart-empty-fa"
                                                                        id="clickme"> 0</class>
                                                                </a>
                                                                {{!-- <span class="love-text">Do you like it?</span><a
                                                                        href="#" class="mfn-love"
                                                                        data-id="clickme"><span class="icons-wrapper"><i
                                                                                class="icon-heart-empty-fa"></i><i
                                                                                class="icon-heart-fa"></i></span><span
                                                                            class="label">122</span></a> --}}
                                                            </div>

                                                            {{!-- <div class="post-links">
                                                                    <i class="icon-doc-text"></i><a
                                                                        href="single-post-4.html" class="post-more">Read
                                                                        more</a>
                                                                </div> --}}
                                                        </div>
                                                        {{/each}}
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>



                        <Footer>
                            <div class="footer_copy">
                                <div class="container">
                                    <div class="column one">
                                        <a id="back_to_top" href="#" class="button button_left button_js"><span
                                                class="button_icon"><i class="icon-up-open-big"></i></span></a>
                                        <div class="copyright">
                                            &copy; Wikifoodia
                                        </div>
                                        <!--Social info area-->
                                        <ul class="social"></ul>
                                    </div>
                                </div>
                            </div>
                        </footer>
                        <script type="text/javascript" src="/jsApp/members.js"></script>
                        <script src="js/jquery-2.1.4.min.js"></script>

                        <script src="js/mfn.menu.js"></script>
                        <script src="js/jquery.plugins.js"></script>
                        <script src="js/jquery.jplayer.min.js"></script>
                        <script src="js/animations/animations.js"></script>
                        <script src="js/translate3d.js"></script>
                        <script src="js/scripts.js"></script>


                        <script src="plugins/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
                        <script src="plugins/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>

                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.video.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.slideanims.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.actions.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.layeranimation.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.kenburn.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.navigation.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.migration.min.js">
                        </script>
                        <script src="plugins/rs-plugin/js/extensions/revolution.extension.parallax.min.js">
                        </script>

                        <script src="http://maps.google.com/maps/api/js?sensor=false&ver=5.9"></script>
                        <script src="js/email.js"></script>



                        </body>

</html>